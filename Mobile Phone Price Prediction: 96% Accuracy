    "# Define parameter distribution for RandomizedSearchCV\n",
    "param_distributions = {\n",
    "    'n_estimators': randint(50, 200),\n",
    "    'max_depth': randint(3, 5),\n",
    "    'min_samples_split': randint(5, 10),\n",
    "    'learning_rate': [0.01, 0.05, 0.1],\n",
    "    'subsample': [0.8, 0.9]\n",
    "}\n",
    "\n",
    "# Perform RandomizedSearchCV\n",
    "random_search = RandomizedSearchCV(\n",
    "    GradientBoostingClassifier(random_state=42, validation_fraction=0.2, n_iter_no_change=10),\n",
    "    param_distributions,\n",
    "    n_iter=10,\n",
    "    cv=5,\n",
    "    verbose=1,\n",
    "    n_jobs=-1,\n",
    "    random_state=42\n",
    ")\n",
    "random_search.fit(X_train, y_train)\n",
    "\n",
    "# Get best model and evaluate\n",
    "best_model_gb = random_search.best_estimator_\n",
    "print(\"Best Parameters for Gradient Boosting:\", random_search.best_params_)\n",
    "print(\"Best Score for Gradient Boosting:\", random_search.best_score_)\n",
    "\n",
    "y_train_pred_gb = best_model_gb.predict(X_train)\n",
    "y_val_pred_gb = best_model_gb.predict(X_val)\n",
    "\n",
    "print(f'Gradient Boosting - Training Accuracy: {accuracy_score(y_train, y_train_pred_gb)}')\n",
