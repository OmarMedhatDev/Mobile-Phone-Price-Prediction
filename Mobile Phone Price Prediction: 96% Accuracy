    "# Create a function for consistent plotting\n",
    "def plot_distribution(data, columns, n_rows, n_cols):\n",
    "    fig, axs = plt.subplots(n_rows, n_cols, figsize=(15, 5*n_rows))\n",
    "    axs = axs.flatten()\n",
    "    \n",
    "    for i, col in enumerate(columns):\n",
    "        sns.histplot(data[col], kde=True, ax=axs[i], color='skyblue', edgecolor='black')\n",
    "        axs[i].set_title(f'Distribution of {col}')\n",
    "        axs[i].set_xlabel(col)\n",
    "        axs[i].set_ylabel('Frequency')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Plot distributions of key features\n",
    "plot_distribution(train, ['px_height', 'px_width', 'battery_power', 'ram'], 2, 2)\n",
    "\n",
    "# Correlation analysis\n",
    "correlation_matrix = train.corr()\n",
    "correlation_with_price = correlation_matrix['price_range'].drop('price_range').sort_values(ascending=False).reset_index()\n",
    "correlation_with_price.columns = ['Feature', 'Correlation with Price Range']\n",
    "\n",
    "fig = px.bar(correlation_with_price,\n",
    "             x='Feature',\n",
    "             y='Correlation with Price Range',\n",
    "             title='Correlation of Features with Price Range',\n",
    "             labels={'Correlation with Price Range': 'Correlation Coefficient'},\n",
    "             color='Correlation with Price Range',\n",
    "             color_continuous_scale='viridis')\n",
    "\n",
    "fig.update_layout(\n",
    "    xaxis_title='Feature',\n",
    "    yaxis_title='Correlation Coefficient',\n",
    "    xaxis=dict(tickangle=90)\n",
    ")\n",
    "\n",
    "fig.show()"
