    "# Create new features\n",
    "def engineer_features(df):\n",
    "    df['screen_area'] = df['px_height'] * df['px_width']\n",
    "    df['aspect_ratio'] = df['px_height'] / df['px_width']\n",
    "    df['performance_score'] = df['ram'] * df['clock_speed'] * df['n_cores']\n",
    "    df['battery_efficiency'] = df['battery_power'] / (df['sc_h'] * df['sc_w'])\n",
    "    df['total_camera_res'] = df['pc'] + df['fc']\n",
    "    df['storage_per_ram'] = df['int_memory'] / df['ram']\n",
    "    df['connectivity_score'] = df['blue'] + df['wifi'] + df['dual_sim'] + df['three_g'] + df['four_g']\n",
    "    df['luxury_score'] = df['touch_screen'] + df['four_g'] + df['wifi'] + (df['ram'] > df['ram'].median())\n",
    "    df['log_battery'] = np.log1p(df['battery_power'])\n",
    "    df['proc_power'] = df['n_cores'] * df['clock_speed']\n",
    "    return df\n",
    "\n",
    "train = engineer_features(train)\n",
    "test = engineer_features(test)\n",
    "\n",
    "# Visualize correlations with new features\n",
    "correlation_matrix = train.corr()\n",
    "correlation_with_price = correlation_matrix['price_range'].drop('price_range').sort_values(ascending=False).reset_index()\n",
    "correlation_with_price.columns = ['Feature', 'Correlation with Price Range']\n",
    "\n",
    "fig = px.bar(correlation_with_price,\n",
    "             x='Feature',\n",
    "             y='Correlation with Price Range',\n",
    "             title='Correlation of Features with Price Range (Including Engineered Features)',\n",
    "             labels={'Correlation with Price Range': 'Correlation Coefficient'},\n",
    "             color='Correlation with Price Range',\n",
    "             color_continuous_scale='viridis')\n",
    "\n",
    "fig.update_layout(\n",
    "    xaxis_title='Feature',\n",
    "    yaxis_title='Correlation Coefficient',\n",
    "    xaxis=dict(tickangle=90)\n",
    ")\n",
    "\n",
    "fig.show()"
   ]
